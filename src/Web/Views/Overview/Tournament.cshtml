@model Web.Models.LeaguesOverviewModel

@{
    ViewBag.Title = "League Overview";
    if (Request.IsAuthenticated)
    {
        Layout = "~/Views/Shared/_Dashboard.cshtml";
    }
    else
    {
        Layout = "~/Views/Shared/_Layout.cshtml";
    }
}
<a id="tournaments"></a>
<div id="tournament-list">
        @Html.Partial("_InfoOverview", Model.Tournaments, new ViewDataDictionary(this.ViewData) { { "header", "<h1>Tournaments</h1>" } })
        @Html.Partial("_InfoOverviewOthers", Model.Other, new ViewDataDictionary(this.ViewData) { { "header", "<h1>Other</h1>" } })
</div>


